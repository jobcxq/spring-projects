<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="Animal" class="com.study.spring.vo.Animal">
		<property name="name" value="dog"></property>
	</bean>

	<alias name="User" alias="Student"></alias>

	<bean id="User" class="com.study.spring.vo.User">
		<property name="name" value="chenxiaoqin"></property>
	</bean>
	

</beans>